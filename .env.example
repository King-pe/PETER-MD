# PETER-MD Configuration
# Copy this file to .env and fill in your values

# WhatsApp Session ID (Get from the web interface)
SESSION_ID=

# MongoDB Connection String
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority

# Bot Configuration
PREFIX=.
MODE=public
BOT_NAME=PETER-MD
OWNER_NAME=PETER

# Owner and Sudo Numbers (with country code)
OWNER_NUMBER=255682211773
SUDO=255682211773

# Optional: Render App URL (for deployment)
APP_URL=https://your-app-name.onrender.com

# Optional: Timezone
TZ=Africa/Dar_es_Salaam

# Optional: API Keys
OPENAI_API_KEY=
REMOVE_BG_KEY=

# Optional: Features
WELCOME=false
GOODBYE=false
AUTO_READ_STATUS=true
AUTO_SAVE_STATUS=false
